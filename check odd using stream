import java.util.ArrayList;
import java.util.stream.IntStream;
import java.util.stream.Stream;
import java.util.List;
import java.util.stream.Collectors;


class HelloWorld {
    public static void main(String[] args) {
        System.out.println("Try programiz.pro");
       ArrayList<Integer> intt = new  ArrayList<Integer>();
         intt.add(118);
         intt.add(11);
          intt.add(1);
          
          System.out.println( intt.stream().allMatch(x-> x%2!=0 ) );
          
          String s="yyyy";
          System.out.println( s.toLowerCase().matches(".*[aeiou].*" ) );
          
          IntStream test= s.chars();
          Stream<Character> testStream= test.mapToObj(c->(char)c );
          List<Character> testchar = testStream.collect(Collectors.toList());
          System.out.println(testchar);
          
          
          
    
    }
}
